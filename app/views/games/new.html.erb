<h1>Games#new</h1>
<% yield %>
<%= @new %>
<h4>Insert Your Answer</h4>
<form action="/score" method="post">
  <%= hidden_field_tag :letters, @new %>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <input type="text" name="guess" placeholder="Your Answer">
  <input type="submit">
</form>
